	xmlns:v="urn:import:com.vaadin.ui"
	xmlns:f="urn:import:com.evidence.fe.form"
	xmlns:i="urn:org.vaadin.mvp.uibinder.method"
	v:sizeFull="" v:spacing="true" v:margin="true">

	<v:Panel ui:field="container">
		<f:EvidenceForm ui:field="kindergartenForm">
			<i:addFooter>
				<v:HorizontalLayout spacing="true">
