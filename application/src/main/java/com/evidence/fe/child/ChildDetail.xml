	xmlns:v="urn:import:com.vaadin.ui"
	xmlns:f="urn:import:com.evidence.fe.child"
	xmlns:i="urn:org.vaadin.mvp.uibinder.method"
	v:sizeFull="" v:spacing="true" v:margin="true">

	<f:ChildDetailForm ui:field="childForm">
